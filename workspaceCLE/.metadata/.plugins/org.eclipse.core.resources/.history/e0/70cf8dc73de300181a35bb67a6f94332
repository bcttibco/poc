                            <bpws:transitionCondition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath2.0"><![CDATA[count($get-canonical-errors/pfx:canonical-error[pfx:code = $Start/pfx:canonical-error/pfx:code]) > 0]]></bpws:transitionCondition>
